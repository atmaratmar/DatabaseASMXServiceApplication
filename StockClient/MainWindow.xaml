<Window x:Class="StockClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StockClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label Content="Select stock:" HorizontalAlignment="Left" Margin="30,25,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.107,-0.692"/>
        <TextBox x:Name="textBox_ISO" HorizontalAlignment="Left" Height="23" Margin="30,56,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="textBox_ISO_TextChanged"/>
        <Label x:Name="Label_Name" Content="" HorizontalAlignment="Left" Margin="314,56,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Label_ISO" Content="" HorizontalAlignment="Left" Margin="314,101,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Label_Value" Content="" HorizontalAlignment="Left" Margin="314,141,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
